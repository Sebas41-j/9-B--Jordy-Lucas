Algoritmo Determinar_Positivo_Negativo
	Definir n Como Real;
	Escribir "Ingresa un número:";
	Leer n;
	Si n > 0 Entonces
		Escribir "El número es positivo.";
	Sino
		Si n < 0 Entonces
			Escribir "El número es negativo.";
		Sino
			Escribir "El número es cero.";
		FinSi
	FinSi
FinAlgoritmo
